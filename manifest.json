  {
    "name": "Linkfish",
    "version": "1.0",
    "description": "Linkfish is a bookmark manager for Google Chrome.",
    "author": "moev.biz",
    "permissions": ["activeTab", "bookmarks", "storage"],
    "chrome_url_overrides": {  
        "newtab": "newtab.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "manifest_version": 2
  }